{
    "name": "node-red-docker",
    "version": "3.0.2",
    "description": "Low-code programming for event-driven applications",
    "homepage": "http://nodered.org",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/node-red/node-red-docker.git"
    },
    "main": "node_modules/node-red/red/red.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug": "node --inspect=0.0.0.0:9229 $NODE_OPTIONS node_modules/node-red/red.js $FLOWS",
        "debug_brk": "node --inspect=0.0.0.0:9229 --inspect-brk $NODE_OPTIONS node_modules/node-red/red.js $FLOWS"
    },
    "dependencies": {
        "node-red": "3.0.2",
        "node-red-contrib-ethereum": "https://github.com/timweing/node-red-contrib-ethereum",
        "node-red-contrib-ethers": "https://github.com/hybuild-project/node-red-contrib-ethers",
        "node-red-contrib-watchdirectory": "https://github.com/fatoldsun00/node-red-contrib-watchdirectory",
        "node-red-contrib-fiware_official": "git+https://github.com/FIWARE/node-red-contrib-FIWARE_official.git#master",
        "passport-fiware-oauth": "^0.3.0"
    },
    "engines": {
        "node": ">=16"
    }
}